{
  "manifest_version": 3,
  "name": "Weather Extension",
  "version": "1.0",
  "description": "Get weather details for a specific city",

  "host_permissions": [
    "https://api.openweathermap.org/*",
    "http://localhost/*"
  ],
  "action": {
    "default_popup": "index.html"
    
  }
  
}
